doctype html
//https://pug.bootcss.com/api/getting-started.html

html
    head
        block head
            script(src='edit.js')
            link(href='edit.css' type='text/css' rel='stylesheet')

    body
        p(id="fileName" hidden=true title=fileName)
        table(id='wrapper')
            tr
                td
                    img(id="image" style="max-width:100%;cursor:pointer;" onclick="editDiagram(this);"
                    src=imageData)
